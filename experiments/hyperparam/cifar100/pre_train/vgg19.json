{
    "dataloader_seed": 200,
    "model_type": "vgg-19",
    "dataset": "cifar100",
    "batch_size": 128,
    "validation_size": 0.1,

    "pre_train": true,
    "epochs": 200,
    "lr": 0.1,
    "lr_drops": [
        60,
        120,
        160
    ],
    "lr_drop_rate": 0.2,
    "optimizer": "nesterov",
    "weight_decay": 0.0005
}